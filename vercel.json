{
  "buildCommand": "cd client && npm run build && cd ../api && npm run build",
  "outputDirectory": "client/dist",
  "installCommand": "bash install-vercel.sh",
  "framework": null,
  "functions": {
    "api/index.ts": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.ts"
    }
  ]
}

